import React from 'react';
declare type ValueInputProps = {
    id?: string;
    value: string;
    innerLabel?: false | React.ReactNode;
    type?: 'number' | undefined;
    onUpdate: (value: any) => void;
    onChange: (value: string) => void;
    onKeyDown?: (event: React.KeyboardEvent) => void;
};
export declare function ValueInput({ innerLabel, value, onUpdate, onChange, onKeyDown, type, id, ...props }: ValueInputProps): JSX.Element;
export declare function NumberInput({ onUpdate, ...props }: ValueInputProps): JSX.Element;
export {};
