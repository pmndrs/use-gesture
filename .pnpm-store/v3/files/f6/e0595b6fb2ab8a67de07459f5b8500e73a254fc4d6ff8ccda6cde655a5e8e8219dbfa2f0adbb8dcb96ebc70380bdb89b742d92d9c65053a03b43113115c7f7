import { FunctionComponent, ImgHTMLAttributes } from "react";
import { SourceProps } from "./picture";
export declare type PlaceholderProps = ImgHTMLAttributes<HTMLImageElement> & {
    fallback?: string;
    sources?: Array<SourceProps>;
};
export declare const Placeholder: FunctionComponent<PlaceholderProps>;
