import React from 'react';
import { ValueInput } from '../ValueInput';
import type { StringProps } from './string-types';
declare type BaseStringProps = Pick<StringProps, 'displayValue' | 'onUpdate' | 'onChange'> & Omit<React.ComponentProps<typeof ValueInput>, 'value'>;
export declare function String({ displayValue, onUpdate, onChange, ...props }: BaseStringProps): JSX.Element;
export declare function StringComponent(): JSX.Element;
export {};
